<UserControl x:Class="ImageMorpher.ImageContainer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ImageMorpher"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="225">
    <Grid>
        <Image Name="image" HorizontalAlignment="Left" Height="400" VerticalAlignment="Top" Width="225"/>
        <Canvas Name="canvas" HorizontalAlignment="Left" Height="400" VerticalAlignment="Top" Width="225"/>
    </Grid>
</UserControl>
